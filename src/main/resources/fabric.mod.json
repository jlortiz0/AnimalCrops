{
  "schemaVersion": 1,
  "id": "animalcrops",
  "version": "${version}",

  "name": "Animal Crops",
  "description": "Grow animals from seeds",
  "authors": [
    "KnightMiner",
    "jlortiz0"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/animal-crops",
    "sources": "https://github.com/KnightMiner/AnimalCrops"
  },

  "license": "MIT",
  "icon": "assets/animalcrops/icon.png",

  "environment": "*",
